<%= link_to "Home", posts_path %>
<%= link_to "Sign out", destroy_user_session_path, method: :delete %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>URL</th>
      <th>Description</th>
      <th>Tags</th>
    </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= link_to post.url, post.url %></td>
      <td><%= post.description %></td>
      <td><%= post.tags %></td>
    </tr>
  <% end %>
  </tbody>
</table>
